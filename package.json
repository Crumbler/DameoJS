{
  "name": "dameojs",
  "version": "1.0.0",
  "description": "An implementation of Dameo in JavaScript",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "check": "tsc --build",
    "build": "tsc --build && vite build",
    "test": "vitest --config src/scripts/tests/vitest.config.ts",
    "test-ci": "vitest --watch=false --config src/scripts/tests/vitest.config.ts"
  },
